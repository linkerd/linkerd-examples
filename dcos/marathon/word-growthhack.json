{
  "id": "/word-growthhack",
  "instances": 3,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "gobsvc/word:growthhack",
      "forcePullImage": false,
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 0,
          "hostPort": 0,
          "servicePort": 0,
          "protocol": "tcp"
        }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/"
    }
  ],
  "cmd": "/usr/bin/gobwordsvc --srv=:$PORT0",
  "cpus": 0.25,
  "mem": 256.0,
  "uris": [
    "file:///etc/docker.tar.gz"
  ]
}
