{
  "id": "/word",
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "gobsvc/gob:0.8.6",
      "forcePullImage": false,
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 0,
          "hostPort": 0,
          "servicePort": 0,
          "protocol": "tcp"
        }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "TCP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "cmd": "word --srv=:$PORT0",
  "cpus": 0.25,
  "mem": 256.0
}
